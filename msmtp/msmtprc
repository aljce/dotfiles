defaults
auth     on
tls      on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile  /var/log/msmtp.log

account  personal
host     smtp.gmail.com
port     587
from     mckean.kylej@gmail.com
user     mckean.kylej
passwodeval "gpg2 --quiet --for-your-eyes-only --not-tty --decrypt ~/.password-store/mail/personal.gpg"
